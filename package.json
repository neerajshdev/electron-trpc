{
  "name": "@neerajshdev/electron-trpc",
  "version": "1.0.1",
  "description": "tRPC integration for Electron with IPC communication",
  "keywords": [
    "electron",
    "trpc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/neerajshdev/electron-trpc"
  },
  "license": "ISC",
  "author": "neerajshdev@github.com",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/mainHandler.js",
  "types": "./dist/mainHandler.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mainHandler.d.ts",
      "default": "./dist/mainHandler.js"
    },
    "./mainHandler": {
      "types": "./dist/mainHandler.d.ts",
      "default": "./dist/mainHandler.js"
    },
    "./ipcLink": {
      "types": "./dist/ipcLink.d.ts",
      "default": "./dist/ipcLink.js"
    },
    "./expose": {
      "types": "./dist/expose.d.ts",
      "default": "./dist/expose.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "publish": "npm run build && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "electron": "^40.0.0",
    "typescript": "^5.9.3",
    "@trpc/client": "^11.8.1",
    "@trpc/server": "^11.8.1"
  }
}
